<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- ==================================== scss ================================= -->
    <template id="frontend_website_style_customize" name="Frontend Website Style"
              inherit_id="website.assets_frontend" active="True">
        <xpath expr="." position="inside">
            <link href="/corby_theme/static/src/scss/theme_common.scss"
                  type="text/scss" rel="stylesheet"/>
        </xpath>
    </template>

    <!-- =============== Footer Style CONTAINER ================= -->


    <template id="footer_default" name="footer_default"
              inherit_id="website.assets_frontend" active="True">
        <xpath expr="." position="inside">
            <link href="/corby_theme/static/src/scss/layouts/footer_default.scss" rel="stylesheet"
                  type="text/scss"/>
        </xpath>
    </template>

</odoo>