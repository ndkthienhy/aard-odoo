    <template id="website.layout_footer_copyright" inherit_id="website.layout"
              name="Footer Copyright">
        <xpath expr="//footer" position="inside">
            <div class="copyright-footer">
                <div class="container" style="background: #272723;">
                    <div class="row">
                        <div class="text-white text-center mt16 mb16 col-md-12"
                             itemscope="itemscope"
                             itemtype="http://schema.org/Organization"
                             style="padding-left:16px">
                            Copyright &amp;copy;
                            <span t-field="res_company.name" itemprop="name">
                                Company name
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </xpath>
    </template>

